<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.LatihanGradle.fajar.dataMaster.model.pelanggan.Pelanggan" table="data_pelanggan">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="idPelanggan" type="long" column="id">
         <generator class="native"/>
      </id>
      <property name="namaPelanggan" column="nama" type="string"/>
      <property name="alamatPelanggan" column="alamat" type="string"/>
      <property name="emailPelanggan" column="email" type="string"/>
      <property name="noHpPelanggan" column="noHp" type="string"/>
   </class>
</hibernate-mapping>